block modal-result
	#resultModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='resultModalLabel', data-backdrop='false')
		.modal-dialog.modal-lg(role='document')
			.modal-content
				.modal-header
					button.close(type='button', data-dismiss='modal', aria-label='Close')
						span(aria-hidden='true') Ã—
					h2#resultModalLabel.modal-title
				.modal-body.row
					#correctBody
						.col-xs-12
							h3 Correct!
					#incorrectBody
						.col-xs-12
							label(for='inputBox') Input
							pre#inputBox

						.col-xs-12.col-md-6
							label(for='outputBox') Your output
							pre#outputBox
						.col-xs-12.col-md-6
							label(for='answerBox') Answer output
							pre#answerBox
					#compileErrorBody
						.col-xs-12
							pre#compileErrorBox
					#runtimeErrorBody
						.col-xs-12
							label(for='runtimeErrorSignal') Return value of #[code main()]
							pre#runtimeErrorSignal

						.col-xs-12
							label(for='runtimeErrorBox') Error log
							pre#runtimeErrorBox

						.col-xs-12
							label(for='runtimeErrorInputBox') Input
							pre#runtimeErrorInputBox
					#timeoutBody
						.col-xs-12
							label(for='timeoutInputBox') Input
							pre#timeoutInputBox
					#errorBody
						.col-xs-12
							h3#errorBox
				.modal-footer
					button.btn.btn-default(type='button', data-dismiss='modal') Close